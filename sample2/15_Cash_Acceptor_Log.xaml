                        </Grid.ColumnDefinitions>
                        <StackPanel Grid.Column="0" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="2" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="4" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="6" Width="2" Background="Black"/>
                        <TextBlock Grid.Column="1" VerticalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" FontSize="20" FontWeight="Bold">2000x</TextBlock>
                        <TextBox x:Name="Credit_2000" Grid.Column="3" TextChanged="Credit_TextChanged" PreviewTextInput="number_PreviewTextInput" Margin="10" TextAlignment="Center" TextWrapping="Wrap" FontSize="20"/>
                        <TextBox x:Name="Debit_2000" Grid.Column="5" TextChanged="Debit_TextChanged" PreviewTextInput="number_PreviewTextInput" Margin="10" TextAlignment="Center" TextWrapping="Wrap" FontSize="20"/>

                    </Grid>
                    <Grid Grid.Row="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
