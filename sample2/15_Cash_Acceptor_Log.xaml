                    <StackPanel Grid.Row="4" Height="2" Background="Black"/>
                    <StackPanel Grid.Row="6" Height="2" Background="Black"/>
                    <StackPanel Grid.Row="8" Height="2" Background="Black"/>
                    <StackPanel Grid.Row="10" Height="2" Background="Black"/>
                    <StackPanel Grid.Row="12" Height="2" Background="Black"/>
                    <StackPanel Grid.Row="14" Height="2" Background="Black"/>
                    <StackPanel Grid.Row="16" Height="2" Background="Black"/>
                    <StackPanel Grid.Row="18" Height="2" Background="Black"/>


                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="1.5*" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="auto" />

                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="auto" />
                        </Grid.ColumnDefinitions>
                        <StackPanel Grid.Column="0" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="2" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="4" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="6" Width="2" Background="Black"/>

                        <TextBlock Grid.Column="1" VerticalAlignment="Bottom" TextAlignment="Center" TextWrapping="Wrap" FontSize="20" FontWeight="Bold" HorizontalAlignment="Left" Width="235">Denomination</TextBlock>
                        <TextBlock Grid.Column="3" VerticalAlignment="Top" TextAlignment="Center" TextWrapping="Wrap" FontSize="20" FontWeight="Bold">Credit</TextBlock>
                        <TextBlock Grid.Column="5" VerticalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" FontSize="20" FontWeight="Bold">Debit</TextBlock>
