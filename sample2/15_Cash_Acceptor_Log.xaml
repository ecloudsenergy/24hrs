                            <ColumnDefinition Width="36*"/>
                            <ColumnDefinition Width="60*"/>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="156*" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="157*" />
                            <ColumnDefinition Width="auto" />
                        </Grid.ColumnDefinitions>
                        <StackPanel Grid.Column="0" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="4" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="6" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="8" Width="2" Background="Black"/>
                        <TextBlock Grid.Column="1" VerticalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" FontSize="20" FontWeight="Bold" Grid.ColumnSpan="3" Margin="0,11">500x</TextBlock>
                        <TextBox x:Name="Credit_500" Grid.Column="5" Margin="10"  TextChanged="Credit_TextChanged" PreviewTextInput="number_PreviewTextInput" VerticalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" FontSize="20"></TextBox>
                        <TextBox x:Name="Debit_500" Grid.Column="7" Margin="10" TextChanged="Debit_TextChanged" PreviewTextInput="number_PreviewTextInput" TextAlignment="Center" TextWrapping="Wrap" FontSize="20"/>
                    </Grid>
