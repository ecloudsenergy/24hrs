<Page x:Class="sample2._15_Cash_Acceptor_Log"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:sample2"
      mc:Ignorable="d" 
      d:DesignHeight="1000" d:DesignWidth="1000"
      Title="_15_Cash_Acceptor_Log">

    <Page.Resources>

        <Style TargetType="Button" >
            <Setter Property="BorderThickness" Value="4"/>
            <Setter Property="FontSize" Value="26"/>
            <Setter Property="FontWeight" Value="ExtraBold"/>
            <Setter Property="FontFamily" Value="Baskerville Old Face"/>
            <Setter Property="Foreground" Value="OliveDrab"/>
            <Setter Property="Height" Value="60"/>
            <Setter Property="Width" Value="200"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border CornerRadius="10" BorderBrush="OliveDrab" BorderThickness="{TemplateBinding BorderThickness}" >
                            <Grid Background="LightGray">
                                <ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}" />
                            </Grid>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="Button.Effect">
                <Setter.Value>
                    <DropShadowEffect Color="Black" Direction="320" ShadowDepth="3" BlurRadius="5" Opacity="0.5" />
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="BorderThickness" Value="4"/>
                    <Setter Property="FontSize" Value="30"/>
                    <Setter Property="FontWeight" Value="ExtraBold"/>
                    <Setter Property="FontFamily" Value="Baskerville Old Face"/>
                    <Setter Property="Foreground" Value="White"/>
                    <Setter Property="Height" Value="60"/>
                    <Setter Property="Width" Value="200"/>
                    <Setter Property="Template">

                        <Setter.Value>
                            <ControlTemplate TargetType="Button">
                                <Border CornerRadius="10" BorderBrush="OliveDrab" BorderThickness="{TemplateBinding BorderThickness}">
                                    <Grid Background="OliveDrab">
                                        <ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}" />
                                    </Grid>

                                </Border>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>

                </Trigger>

            </Style.Triggers>

        </Style>
    </Page.Resources>
    <Grid Background="White">

        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Canvas >
                <Image Source="/images/24hrs Yellow with green background.png" 
                       Height="100" Width="200" Canvas.Right="10" Canvas.Top="10"/>
                <Image Source="/images/eClouds Logo.png" Height="100" Width="200" 
                       Canvas.Left="20" Canvas.Top="10"/>
            </Canvas>
            <TextBlock HorizontalAlignment="Center" Margin="0,10,0,0" FontSize="35" 
                       FontWeight="ExtraBold" Foreground="Maroon" TextDecorations="Underline" > 
                Note Acceptor - Cash Removal</TextBlock>
        </Grid>
        <Grid Grid.Row="2">

            <Button HorizontalAlignment="Left" Margin="100,-59,0,0" Content="Back" Click="Back_Button"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,0,100,0">
                <Button Margin="0,-25,49,0" Content="Save" Click="Save_Button" VerticalAlignment="Top"/>
                <Popup Name="Verify_popup"

               Placement="Center"

               HorizontalOffset="-300"

               VerticalOffset="-300"

               AllowsTransparency="True">

                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>


                        <Border Height="450" Width="700"
            CornerRadius="6"
                BorderBrush="OliveDrab"
                BorderThickness="3"
                Padding="2" 
                    Background="LightGray" VerticalAlignment="Bottom" Margin="190,-15,300,49" Grid.RowSpan="2" Grid.ColumnSpan="2">


                            <ScrollViewer Grid.Row="1" Background="Transparent"  PanningMode="VerticalOnly" VerticalScrollBarVisibility="Auto" 
                             Focusable="False" HorizontalScrollBarVisibility="Disabled" >
                                <StackPanel Grid.Row="0" x:Name="Confirm_list" Width="600"  Orientation="Vertical">

                                </StackPanel>



                            </ScrollViewer>



                            <!--<Button Grid.Row="1" Width="80" x:Name="close" Height="40" FontSize="14" Content="Close" Click="close_Click"/>-->

                        </Border>




                        <Button Click="Hide_Click" Margin="227,476,755,6" Grid.Row="1" Grid.Column="1" Height="Auto">Cancel</Button>
                        <Button Click="Confirm_Click" Margin="621,476,365,6" Grid.Row="1" Grid.Column="1" Height="Auto" >Confirm</Button>


                    </Grid>

                </Popup>
                <Button Margin="10,-59,2,0" Content="Reset" Click="Reset_Button"/>
            </StackPanel>
        </Grid>


        <Grid Grid.Row="1" Margin="0,10,0,115" Grid.RowSpan="3" VerticalAlignment="Center">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="5*" />
                <ColumnDefinition Width="2*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>


            <StackPanel Grid.Column="2" Orientation="Vertical" Margin="15,5,15,5" VerticalAlignment="Center">
                <TextBlock FontSize="22" FontWeight="Bold" TextAlignment="Center" Foreground="Maroon">Request Type</TextBlock>
                <Border Grid.Column="2" Grid.Row="3"  VerticalAlignment="Center" CornerRadius="6" BorderBrush="Gray" 
                        Background="White" BorderThickness="4" >
                    <ComboBox x:Name="Request_type" FontSize="30"  FontFamily="Baskerville Old Face" SelectionChanged="Status_Txt_SelectionChanged">
                        <ComboBoxItem IsSelected="True" Foreground="Black">Select Option</ComboBoxItem>
                        <ComboBoxItem Foreground="Black" >Removal</ComboBoxItem>
                        <ComboBoxItem Foreground="Black" >Remove and Credit</ComboBoxItem>
                        <ComboBoxItem Foreground="Black" >Credit</ComboBoxItem>
                    </ComboBox>
                </Border>
                <TextBlock FontSize="22" FontWeight="Bold" TextAlignment="Center" Foreground="Maroon">Remark</TextBlock>
                <Border Grid.Column="2" Grid.Row="3"  VerticalAlignment="Center" CornerRadius="10" BorderBrush="Gray" 
                        Background="White" BorderThickness="4" >
                    <TextBox x:Name="Remark" FontSize="22" TextAlignment="Right" Foreground="Maroon" Height="52" TextChanged="REMARK_TextChanged"></TextBox>
                </Border>
                <TextBlock FontSize="22" FontWeight="Bold" TextAlignment="Center" Foreground="Maroon">Total Credit</TextBlock>
                <Border Grid.Column="2" Grid.Row="3"  VerticalAlignment="Center" CornerRadius="6" BorderBrush="Gray" 
                        Background="White" BorderThickness="4" >
                    <TextBox x:Name="Total_Credited" FontSize="22" TextAlignment="Right" Foreground="Maroon" TextChanged="Total_Credited_TextChanged" IsReadOnly="True"></TextBox>
                </Border>
                <TextBlock FontSize="22" FontWeight="Bold" TextAlignment="Center" Foreground="Maroon" >Total Debit</TextBlock>
                <Border Grid.Column="2" Grid.Row="3"  VerticalAlignment="Center" CornerRadius="6" BorderBrush="Gray" 
                        Background="White" BorderThickness="4" >
                    <TextBox x:Name="Total_Debited" FontSize="22" TextAlignment="Right" Foreground="Maroon" TextChanged="Total_Debited_TextChanged" IsReadOnly="True" ></TextBox>
                </Border>




            </StackPanel>


            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="7*"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <StackPanel Orientation="Vertical" Grid.Row="0" >
                    <StackPanel Orientation="Horizontal" >
                        <TextBlock FontSize="22" FontWeight="Bold" Foreground="Maroon">Username:</TextBlock>
                        <TextBlock x:Name="username" Margin="20,0,0,0" FontSize="22" Foreground="Maroon">Username</TextBlock>
                        <TextBlock Margin="50,0,0,0" FontSize="22" FontWeight="Bold" Foreground="Maroon">DateTime:</TextBlock>
                        <TextBlock x:Name="Time_Now" Margin="20,0,0,0" FontSize="22" Foreground="Maroon">00/00/00 15:15</TextBlock>
                    </StackPanel>

                </StackPanel>
                <Grid Grid.Row="1">

                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="auto"/>
