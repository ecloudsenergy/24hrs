                        <TextBox x:Name="Debit_20" Grid.Column="5"  TextChanged="Debit_TextChanged" PreviewTextInput="number_PreviewTextInput" Margin="10" VerticalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" FontSize="20"></TextBox>
