                        <StackPanel Grid.Column="0" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="2" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="4" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="6" Width="2" Background="Black"/>
                        <TextBlock Grid.Column="1" VerticalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" FontSize="20" FontWeight="Bold">10x</TextBlock>
                        <TextBox x:Name="Credit_10" Grid.Column="3"  TextChanged="Credit_TextChanged" PreviewTextInput="number_PreviewTextInput" Margin="10" VerticalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" FontSize="20" ></TextBox>
                        <TextBox x:Name="Debit_10" Grid.Column="5"  TextChanged="Debit_TextChanged" PreviewTextInput="number_PreviewTextInput" Margin="10" TextAlignment="Center" TextWrapping="Wrap" FontSize="20"/>
                    </Grid>
                    <Grid Grid.Row="17">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="1.5*" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="auto" />
                        </Grid.ColumnDefinitions>
                        <StackPanel Grid.Column="0" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="2" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="4" Width="2" Background="Black"/>
                        <StackPanel Grid.Column="6" Width="2" Background="Black"/>
                        <TextBlock Grid.Column="1" VerticalAlignment="Center" TextAlignment="Center" TextWrapping="Wrap" FontSize="20" FontWeight="Bold">5x</TextBlock>
